<Module specificationVersion='2'>
  <ModulePrefs title="Places and Street View">
    <Require feature="dynamic-height"/>
    <Require feature="osapi"/>
    <Locale lang="all">
      <msg name="savePlace">#{obj:actor} saved a place around #{obj:object} from #{obj:target}</msg>
      <msg name="savePlaceContent"><![CDATA[
      	<div>
      	<div>#{obj:object.attachments.1}</div>
      	<div><img style="max-width: 100%; height: auto" src="#{obj:object.attachments.2}"></div>
      	<div><img style="max-width: 100%; height: auto" src="#{obj:object.attachments.3}"></div>
        </div>
      ]]></msg>
    </Locale>
  </ModulePrefs>
  <Content type="html" href="snappy.html"/>
</Module>
