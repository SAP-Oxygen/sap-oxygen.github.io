<Module specificationVersion='2'>
  <ModulePrefs title="Feed_Input_Output">
    <Require feature="dynamic-height"/>
    <Require feature="wave"/>
    <Require feature="osapi"/>
    <Locale lang="all">
      <msg name="addItem">#{obj:actor} added &lt;b&gt;the item '#{obj:object}'&lt;/b&gt; to the bullet list #{obj:target}</msg>
      <msg name="addItemContent"><![CDATA[
          <span style='font-weight: bold'>#{obj:object}</span>
      ]]></msg>
      <msg name="itemLabel"></msg>
    </Locale>
  </ModulePrefs>
  <Content type="html" href="Feed_Input_Output.html"/>
</Module>
